<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".features.services.views.fragments.EndovenosoFragment">

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <include
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/toolbar"
                layout="@layout/services_navigation_toolbar"/>

        <TextView
                android:id="@+id/tv_title"
                android:layout_below="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Endovenoso"/>

        <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rv_item_service_type"
                android:layout_below="@id/tv_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:itemCount="3"
                tools:listitem="@layout/rv_item_service_type">

        </androidx.recyclerview.widget.RecyclerView>

    </RelativeLayout>
</layout>